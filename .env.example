# Azure OpenAI 설정 (필수)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_KEY=your-azure-openai-key-here
AZURE_OPENAI_DEPLOYMENT=your-deployment-name

# Agent 설정
AGENT_NAME=VibeCodingAgent
MODEL=gpt-4
TEMPERATURE=0.7
MAX_TOKENS=2000
TIMEOUT=60

# System Message (선택)
SYSTEM_MESSAGE=You are a helpful AI assistant built with Microsoft Agent Framework.

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=logs/agent.log

# 플러그인 설정
ENABLE_WEB_SEARCH=true
ENABLE_CALCULATOR=true
ENABLE_WEATHER=false

# 외부 API 키 (플러그인 사용 시)
OPENWEATHERMAP_API_KEY=your-weather-api-key-here

# 저장소 설정
STORAGE_TYPE=json  # json, sqlite, redis
STORAGE_PATH=./data/storage

# Redis 설정 (STORAGE_TYPE=redis인 경우)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# 성능 설정
MAX_CONCURRENT_REQUESTS=10
ENABLE_CACHE=true
CACHE_TTL=3600

# 보안 설정
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
ENABLE_RATE_LIMITING=true
RATE_LIMIT_PER_MINUTE=60
